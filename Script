game:GetService("StarterGui"):SetCore("SendNotification", { 
	Title = "Feito por snow";
	Text = "Obrigado por usar meu script <3";
	Icon = "rbxthumb://type=Asset&id=7121294175&w=150&h=150"})
Duration = 10;

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/thaibao/main/Librarykavoui"))()

local Window = Library.CreateLib("Snowzin Hub | Stands awakening            „Ö§„Ö§„Ö§„Ö§„Ö§Made by Snowbrkkkkk", "DarkTheme")

local Tab = Window:NewTab("Stands awakening")
local Section = Tab:NewSection("Use in game")

Section:NewButton("Stand Pose", "LOLLLLLL", function()
-- Desbloqueador de movimento simples
-- Tenta reativar o controle de movimenta√ß√£o do personagem

local player = game.Players.LocalPlayer
local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")

if humanoid then
	humanoid.WalkSpeed = 16
	humanoid.JumpPower = 50
	humanoid.PlatformStand = false
end

-- Se o jogo tentar travar de novo, for√ßa o desbloqueio a cada 0.2s
while true do
	task.wait(0.2)
	pcall(function()
		local character = player.Character
		if character then
			local hum = character:FindFirstChildOfClass("Humanoid")
			if hum then
				hum.WalkSpeed = 16
				hum.JumpPower = 50
				hum.PlatformStand = false
			end
		end
	end)
end
    print("Clicked")
end)

Section:NewButton("Fire button", "nada", function()
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

local function addStrongEffects()
	for _, part in pairs(character:GetDescendants()) do
		if part:IsA("BasePart") and part.Transparency < 1 then
			-- Fogo maior
			local fire = Instance.new("Fire")
			fire.Size = 15 -- tamanho aumentado
			fire.Heat = 25 -- calor visual maior
			fire.Parent = part

			-- Fa√≠scas brilhantes
			local spark = Instance.new("Sparkles")
			spark.SparkleColor = Color3.new(1, 1, 0.5)
			spark.Parent = part

			-- Fuma√ßa densa
			local smoke = Instance.new("Smoke")
			smoke.Color = Color3.new(0.1, 0.1, 0.1)
			smoke.Size = 6 -- mais larga
			smoke.Opacity = 0.7
			smoke.RiseVelocity = 2
			smoke.Parent = part

			-- Luz forte
			local light = Instance.new("PointLight")
			light.Brightness = 3
			light.Range = 20
			light.Color = Color3.new(1, 0.4, 0)
			light.Parent = part
		end
	end
end

addStrongEffects()

-- Reaplica ao respawn
player.CharacterAdded:Connect(function(char)
	char:WaitForChild("HumanoidRootPart")
	wait(1)
	addStrongEffects()
end)
    print("Clicked")
end)

